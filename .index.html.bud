"use strict";

var furColors = require('./lib');
module.exports = {
    force: true,
    mode: '444',
    data: {
        pkg: require('./package.json'),
        get examples() {

            return Object.keys(furColors).map(function (theme) {
                return {
                    colors: furColors[theme](),
                    theme: theme
                };
            })
        }
    }
};
